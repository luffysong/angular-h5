{
    "preset": "./airbnb.json",
    "validateIndentation": 4,
    "requireTrailingComma": false,
    "requireDollarBeforejQueryAssignment": false,
    "safeContextKeyword": [
        "_this",
        "self",
        "vm"
    ],
    "requireCurlyBraces": {
        "keywords": [
            "if",
            "else",
            "for",
            "while",
            "do",
            "try",
            "catch"
        ],
        "allExcept": [
            "return"
        ]
    },
    "requireCamelCaseOrUpperCaseIdentifiers": {
        "ignoreProperties": true,
        "strict": false
    },
    "excludeFiles": [
        "**/dictionary**",
        "**/syndicates**"
    ],
    "maximumLineLength": 1000


}
